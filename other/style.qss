QMainWindow > QWidget
{	
	background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #d0e4f7, stop: 0.24 #73b1e7, stop: 0.5 #0a77d5,  stop: 0.79 #539fe1 , stop: 1 #87bcea);

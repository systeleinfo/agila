{
	background-color: QLinearGradient( x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #aad4ff , stop: 0.05 #99c7ef ,stop:0.49 #88bfef , stop: 0.5 #76b7f7  , stop: 0.95 #68aaed , stop: 1 #80bbf2);

QMainWindow > QStatusBar
{	
	background-color: #87bcea;
	border-top: 2px solid #f5f6f6;	
}

QTabWidget::pane 
{
   	border-top: 2px solid white;
   	border-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #dddfe3, stop: 1 #f5f6f6);
     	position: absolute;
     	top: -0.5em;
}

QTabWidget::tab-bar 
{
     	alignment: center;
}

QTabBar::tab 
{
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #eaeaea, stop: 1.0 #e5e5e5);
	     	
	border: 1px solid #b8bac6;
     	border-bottom-color: #f5f6f6; 
     	border-top-left-radius: 4px;
     	border-top-right-radius: 4px;
     	min-width: 80px;
     	padding: 2px;
	height: 22px;
}

QTabBar::tab:hover 
{
     	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1, stop: 0 #fafafa, stop: 0.4 #f4f4f4, stop: 0.5 #e7e7e7, stop: 1.0 #fafafa);
}

QTabBar::tab:selected  
{
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #ffffff , stop: 1 #eaeaea);
	border: 1px solid #b8bac6;
     	border-top-left-radius: 4px;
     	border-top-right-radius: 4px;
	height: 30px;
}

QTabBar::tab:!selected 
{
     	margin-top: 2px; 
}

QTabBar::tab:selected 
{
     	margin-left: -4px;
     	margin-right: -4px;
}

QTabBar::tab:first:selected 
{
     	margin-left: 0; 
}

QTabBar::tab:last:selected 
{
     	margin-right: 0;
}

QTabBar::tab:only-one 
{
     	margin: 0; 
}

QTableView
{
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #dddfe3, stop: 1 #f5f6f6);
	selection-background-color: #314159;
	gridline-color: transparent;
	border-top-left-radius: 10px;
	border-bottom-right-radius: 10px;
}

QTableView > QTableCornerButton::section 
{
	alternate-background-color: rgb(70, 70, 70); 
	background-color:  rgb(39, 39, 39);
	selection-background-color: red; 
	selection-color: red;
	border-top-left-radius: 10px;
}

QTableView > QHeaderView::section 
{
	alternate-background-color: rgb(70, 70, 70); 
	background: rgb(39, 39, 39); 
	color: rgb(200, 200, 200); 
	selection-background-color: red; 
	selection-color: red;
} 

QTableView > QHeaderView 
{
	alternate-background-color: rgb(70, 70, 70); 
	background: rgb(39, 39, 39); 
	color: rgb(200, 200, 200); 
	selection-background-color: rgb(120, 120, 120); 
	selection-color: rgb(255, 255, 255);
} 

QListWidget 
{
     	show-decoration-selected: 1; 
	background: #f5f6f6;
	border-top-left-radius: 10px;
	border-bottom-right-radius: 10px;
}

QListWidget::item
{
	background: #f5f6f6;
	border-top-left-radius: 10px;
	border-bottom-right-radius: 10px;
}

QListWidget::item:hover 
{
     	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #dddfe3, stop: 1 #f5f6f6);
}

QListView::item:selected:active 
{
     	background: #dddfe3;
	color: grey;
}

QDialogButtonBox > QPushButton 
{	
	min-width: 70px;
}

QSpinBox
{
	border: 1px solid #f5f6f6;
	color: rgb(70, 70, 70);
	min-height: 23px;
}

QSpinBox
{
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #dddfe3, stop: 1 #f5f6f6);
	border-top-left-radius: 10px;
	border-bottom-right-radius: 10px;
}

QSpinBox:disabled
{ 
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #cccdd1, stop: 1 #e5e5e5);
}

QSpinBox[readOnly="true"]
{ 
	background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #cccdd1, stop: 1 #e5e5e5);
}

QSpinBox::up-button 
{
     	subcontrol-origin: border;
     	subcontrol-position: top right;
     	border-left: 1px solid #b8bac6;
     	border-bottom: 1px solid #b8bac6;
     	border-bottom-left-radius: 5px;
     	margin-right: 1px;
     	margin-top: 1px;
}

QSpinBox::up-button:pressed 
{
     	border: 2px solid #dddfe3;
	background: #dddfe3;
}

QSpinBox::up-button:hover
{
     	border: 2px solid #b8bac6;
	background: #b8bac6;
}

QSpinBox::down-button 
{
     	subcontrol-origin: border;
     	subcontrol-position: bottom right; 
     	border-left: 1px solid #b8bac6;
     	border-top: 1px solid #b8bac6;
     	border-top-left-radius: 5px;
     	border-bottom-right-radius: 10px;
     	margin-right: 1px;
     	margin-bottom: 1px;
}

QSpinBox::down-button:pressed 
{
     	border: 2px solid #dddfe3;
	background: #dddfe3;
}

QSpinBox::down-button:hover
{
     	border: 2px solid #b8bac6;
	background: #b8bac6;
}

